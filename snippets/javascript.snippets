snippet cl "console.log"
    console.log("${0}");
snippet ck "console.log with label"
	console.log("${1}:", ${1});
snippet fof
	for (let ${1:elem} of ${2:obj}) {
		${0}
	}
snippet if
	if (${1:condition}) {
		${0}
	}
snippet tc
	try {
		${1}
	} catch (${2:err}) {
		${0}
	}
snippet fn
	function ${1:name}(${2}) {
		${3}
	}
snippet imp
	import ${1:module} from '${2:path}'
